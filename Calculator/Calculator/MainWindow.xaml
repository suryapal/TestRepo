        <Button x:Name="button2" Content="2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Margin="71,47,0,-59" Height="46" FontSize="18"/>
        <Button x:Name="button3" Content="3" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Height="46" FontSize="18" Margin="142,47,0,-59"/>
        <Button x:Name="button4" Content="4" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Height="46" FontSize="18" Margin="1,114,0,-126"/>
        <Button x:Name="button5" Content="5" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Height="46" FontSize="18" Margin="71,112,0,-124"/>
        <Button x:Name="button6" Content="6" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Height="46" FontSize="18" Margin="142,112,0,-124"/>
        <Button x:Name="button7" Content="7" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Height="46" FontSize="18" Margin="1,175,0,-187"/>
