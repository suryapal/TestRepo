        <Button x:Name="button9" Content="9" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Height="46" FontSize="18" Margin="142,180,0,-192"/>
        <Button x:Name="button0" Content="0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Height="46" FontSize="18" Margin="71,242,0,-254"/>
        <Button x:Name="buttonDot" Content="." HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Height="46" FontSize="18" Margin="1,242,0,-254"/>
        <Button x:Name="buttonAdd" Content="+" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Height="46" FontSize="18" Click="buttonAdd_Click" Margin="142,242,0,-254"/>
    </Grid>
</Window>
